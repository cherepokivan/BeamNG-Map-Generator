{
  "name": "beamng-terrain-generator",
  "version": "1.0.0",
  "description": "Generate BeamNG.drive maps from real-world data",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.3",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.9",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/beamng-terrain-generator"
  },
  "keywords": [
    "beamng",
    "terrain",
    "generator",
    "openstreetmap",
    "rust",
    "tauri"
  ],
  "author": "Your Name",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yourusername/beamng-terrain-generator/issues"
  },
  "homepage": "https://github.com/yourusername/beamng-terrain-generator#readme"
}